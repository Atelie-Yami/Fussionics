script/source = "extends Button

@export var blur_path: NodePath
@onready var blur: ColorRect = get_node(blur_path)

@export var pause_menu_path: NodePath
@onready var pause_menu: Control = get_node(pause_menu_path)

var paused: bool


func _ready():
	pause_menu.close_menu.connect(_close_menu)
	pause_menu.Set_ingame()


func _close_menu():
	paused = false
	blur.lod = 0.0


func _is_pressed():
	paused = !paused
	pause_menu.OpenMenuSettings() if paused else pause_menu.close_menu_settings()
	blur.lod = 1.7 if paused else 0.0

"

