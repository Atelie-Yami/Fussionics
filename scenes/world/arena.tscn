[sub_resource type="Shader" id="Shader_h78me"]
code = "shader_type canvas_item;

uniform sampler2D legancy: repeat_enable;
uniform sampler2D border;
uniform vec4 color: source_color;


void fragment() {
	float d = distance(UV, vec2(0.5)) * 2.0;
	float center_desfoque = clamp(pow(d, 6.0), 0.0, 1.0);
	
	vec4 _legancy = texture(legancy, UV + vec2(TIME * 0.05));
	vec4 _border = texture(border, UV);
	vec4 base_color = _legancy * _legancy.a * center_desfoque * (1.0 - pow(_border.a * 1.1, 0.5));
	base_color.rgb *= color.rgb;
	COLOR += clamp(base_color, vec4(0.0), vec4(1.0));
}"

